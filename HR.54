package HRlink2;
import java.util.*;
public class fiftyfour{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int max = Integer.MIN_VALUE;
        for(int i = 0; i < n; i++) {
            int power = sc.nextInt();
            if(power > max) {
                max = power;
            }
        }
        System.out.println(max + " is the maximum element in the array");
    }
}

